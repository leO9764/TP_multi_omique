liste_prot_filtre = protein[, names(which(abs(apply(protein, 2, coef_var)) >= 0.15))]
length(liste_mirna_filtre) # 82
length(liste_mrna_filtre) # 82
length(liste_prot_filtre) # 90
max(liste_mirna_filtre)
